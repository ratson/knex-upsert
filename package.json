{
  "name": "knex-upsert",
  "version": "0.0.4",
  "description": "An upsert function for knex.",
  "scripts": {
    "lint": "run-p lint:*",
    "lint:bundlesize": "bundlesize",
    "lint:js": "eslint --ext js,md .",
    "lint:package.json": "npmPkgJsonLint .",
    "test": "ava",
    "posttest": "npm run lint"
  },
  "dependencies": {
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "bundlesize": "^0.17.0",
    "eslint": "^4.9.1",
    "eslint-config-concise": "^0.26.1",
    "eslint-config-concise-ava": "^0.23.0",
    "knex": "^0.14.6",
    "mock-knex": "^0.4.0",
    "npm-package-json-lint": "^3.1.0",
    "npm-package-json-lint-config-default": "^2.0.0",
    "npm-run-all": "^4.1.3"
  },
  "repository": "ratson/knex-upsert",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "files": [
    "*.md",
    "index.js"
  ],
  "keywords": [
    "knex",
    "upsert"
  ],
  "bundlesize": [
    {
      "path": "./index.js",
      "maxSize": "1 Kb"
    }
  ],
  "npmPackageJsonLintConfig": {
    "extends": "npm-package-json-lint-config-default",
    "rules": {
      "require-keywords": "error"
    }
  }
}
