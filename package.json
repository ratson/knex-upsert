{
  "name": "knex-upsert",
  "version": "0.0.4",
  "description": "An upsert function for knex.",
  "main": "./dist/index.js",
  "scripts": {
    "prepare": "run-s build",
    "build": "tsc -b",
    "lint": "run-p lint:*",
    "lint:size-limit": "size-limit",
    "lint:js": "eslint --ext js,md .",
    "lint:package.json": "npmPkgJsonLint .",
    "test": "ava",
    "posttest": "run-s lint"
  },
  "dependencies": {
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@concise-style/prettier-config": "^0.40.0",
    "@size-limit/preset-small-lib": "^4.9.1",
    "@types/lodash": "^4.14.165",
    "@types/mock-knex": "^0.4.3",
    "@types/node": "^14.14.10",
    "ava": "^3.14.0",
    "eslint": "^7.15.0",
    "eslint-config-concise": "^0.40.0",
    "eslint-config-concise-ava": "^0.40.0",
    "eslint-config-concise-typescript": "^0.40.0",
    "eslint-plugin-prettier": "^3.2.0",
    "husky": "^4.3.5",
    "knex": "^0.21.12",
    "lint-staged": "^10.5.3",
    "mock-knex": "^0.4.9",
    "npm-package-json-lint": "^5.1.0",
    "npm-package-json-lint-config-default": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "size-limit": "^4.9.1",
    "typescript": "^4.1.2"
  },
  "repository": "ratson/knex-upsert",
  "license": "MIT",
  "engines": {
    "node": ">= 10"
  },
  "files": [
    "*.md",
    "index.js",
    "dist/**"
  ],
  "keywords": [
    "knex",
    "upsert"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md}": [
      "eslint --ext js,md --fix"
    ]
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "1 KB"
    }
  ]
}
